{
  "name": "gdc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "postinstall": "npx prisma generate",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:pull": "dotenv -e .env.development.local -- npx prisma db pull",
    "db:push": "dotenv -e .env.development.local -- npx prisma db push",
    "db:generate": "dotenv -e .env.development.local -- npx prisma generate",
    "db:migrate:dev": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "db:migrate:reset": "dotenv -e .env.development.local -- npx prisma migrate reset",
    "db:migrate:introspect": "dotenv -e .env.development.local -- npx prisma migrate dev --name introspected_change",
    "db:seed": "dotenv -e .env.development.local -- npx prisma db seed",
    "studio": "dotenv -e .env.development.local -- npx prisma studio"
  },
  "dependencies": {
    "@nextui-org/react": "2.4.8",
    "@prisma/client": "5.21.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-icons": "1.3.1",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-slot": "1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@sanity/image-url": "1",
    "@sanity/vision": "3",
    "@types/luxon": "3.4.2",
    "@workos-inc/authkit-nextjs": "0.13.1",
    "@workos-inc/node": "7.30.0",
    "autoprefixer": "10.4.20",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "jose": "5.9.6",
    "lodash": "4.17.21",
    "lucide-react": "0.454.0",
    "luxon": "3.5.0",
    "next": "15.0.2",
    "next-sanity": "9.8.8",
    "next-themes": "0.3.0",
    "postcss": "8.4.47",
    "prettier-plugin-tailwindcss": "0.6.8",
    "prisma": "5.21.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sanity": "3",
    "styled-components": "6",
    "tailwind-merge": "2.5.4",
    "tailwindcss": "3.4.14",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "typescript": "5",
    "@types/node": "20",
    "@types/react": "18",
    "@types/react-dom": "18",
    "eslint": "8",
    "eslint-config-next": "15.0.2",
    "prettier": "3.3.3"
  }
}
